<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net6;net7</TargetFrameworks>
    <PackageProjectUrl>https://github.com/secana/PeNet-Analyzer</PackageProjectUrl>
    <Version>0.0.0</Version>
    <Authors>Stefan Hausotte</Authors>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <Description>PeNet.Analyzer finds anomalies in Portable Executables (PE) files.</Description>
    <Copyright>2020 @ Stefan Hausotte</Copyright>
    <RepositoryUrl>https://github.com/secana/PeNet-Analyzer</RepositoryUrl>
    <PackageTags>PE Header Portable Executable Malware</PackageTags>
    <PackageIcon>icon.png</PackageIcon>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>PeNet.snk</AssemblyOriginatorKeyFile>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="PeNet" Version="4.0.2" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1" PrivateAssets="All" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\resource\icon.png" Pack="true" PackagePath="\" />
  </ItemGroup>

</Project>
